   "source": [
    "def make_avarager():\n",
    "    count = 0\n",
    "    total = 0\n",
    "    \n",
    "    def avarager(new_value):\n",
    "        nonlocal count, total\n",
    "        count += 1\n",
    "        total += new_value\n",
    "        return total / count\n",
    "\n",
    "    return avarager"
   ]
  },
